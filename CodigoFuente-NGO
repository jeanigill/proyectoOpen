    <script>
    // POST form data -> gastronomia-form 
$('#gastronomia-form').submit(function(e){
    e.preventDefault();
    $.ajax({
        url:'https://hooks.zapier.com/hooks/catch/4431997/ps22wg/',
        type:'post',
        data:$('#gastronomia-form').serialize(),
        success:function(){
          // Redirect to another success page
          window.location = "http://www.ngosaeca.com.py/gastronomia/rational/muchasgracias";
        }
    });
});
</script>

